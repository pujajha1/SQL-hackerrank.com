#https://www.hackerrank.com/challenges/the-blunder/problem?h_r%5B%5D=next-challenge&h_r%5B%5D=next-challenge&h_v%5B%5D=zen&h_v%5B%5D=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen
select CEIL(AVG(salary)-AVG(replace(to_char(salary),0,'')))
from employees;
