#https://www.hackerrank.com/challenges/earnings-of-employees/problem?h_r%5B%5D=next-challenge&h_r%5B%5D=next-challenge&h_v%5B%5D=zen&h_v%5B%5D=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen&h_r=next-challenge&h_v=zen
select max(months*salary) as SAL,count(name)
from employee
where months*salary=(select max(months*salary) from employee);
